{
    "name": "Portainer",
    "id": "portainer",
    "packaging_format": 1,
    "description": {
        "en": "Portainer is a powerful Container Management System designed for organizations starting out on their Kube journey",
        "fr": "Portainer est un puissant système de gestion de conteneurs conçu pour les organisations qui débutent leur parcours Kube"
    },
    "version": "2.9.3~ynh1",
    "url": "https://portainer.io",
    "upstream": {
        "license": "zlib License",
        "website": "https://portainer.io",
        "demo": "https://www.portainer.io/book-a-demo",
        "admindoc": "https://github.com/portainer/portainer",
        "userdoc": "https://github.com/portainer/portainer",
        "code": "https://github.com/portainer/portainer"
    },
    "license": "zlib License",
    "maintainer": {
        "name": "jarod5001",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/portainer",
                "default": "/portainer"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
